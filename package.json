{
  "name": "ai-content-collector-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for automatically collecting and analyzing AI tutorial content from YouTube and blogs, with automatic Supabase storage",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "ai-content-collector-mcp-server": "dist/index.js"
  },
  "scripts": {
    "build": "tsc && chmod +x dist/index.js && chmod +x dist/crawler.js",
    "dev": "tsx src/index.ts",
    "dev:crawler": "tsx src/crawler.ts",
    "crawler": "node dist/crawler.js",
    "crawler:once": "RUN_ONCE=true node dist/crawler.js",
    "collect": "tsx collect-url.ts",
    "prepare": "npm run build"
  },
  "keywords": [
    "mcp",
    "ai",
    "content",
    "youtube",
    "blog",
    "scraper",
    "supabase"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.4",
    "@supabase/supabase-js": "^2.39.3",
    "axios": "^1.6.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "nanoid": "^5.0.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
